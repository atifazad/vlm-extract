# VLM Extract Configuration
# Copy this file to .env and configure your preferred provider

# VLM Provider Configuration
VLM_PROVIDER=ollama
VLM_BASE_URL=http://localhost:11434
VLM_API_KEY=your_api_key_here
VLM_MODEL=llava
VLM_TIMEOUT=30
VLM_MAX_RETRIES=3

# File Processing Configuration
MAX_FILE_SIZE_MB=50
SUPPORTED_IMAGE_FORMATS=PNG,JPEG,GIF,BMP,WEBP,TIFF,HEIC
SUPPORTED_DOCUMENT_FORMATS=PDF,DOCX,PPTX,XLSX,EPUB,HTML

# Batch Processing Configuration
BATCH_SIZE=5
BATCH_TIMEOUT=60 